{
  "name": "@aigentflow/cli",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "bin": {
    "aigentflow": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "@aigentflow/core": "workspace:*"
  },
  "devDependencies": {
    "@aigentflow/config": "workspace:*",
    "@aigentflow/tsconfig": "workspace:*",
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.0.0"
  }
}
